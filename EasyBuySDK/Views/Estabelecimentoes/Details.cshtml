@model EasyBuySDK.Models.Estabelecimento

@{
    ViewBag.Title = "Details";

}

<div class="cardview" onMouseOver="chamaMarcacaoEndereco(@Json.Encode(Model.Endereco))">
    <h2>Detalhes</h2>

    <h4>Estabelecimento</h4>
    <hr />
    <div class="form-group">

        <div class="form-inline"  style="background-color:#e6e6e6;padding-top:16px;padding-bottom:10px; margin-bottom:16px;" >

            <dl class="dl-horizontal">
                <dt>
                    @Html.DisplayNameFor(model => model.Nome)
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.Nome)
                </dd>

                <dt id="address">
                    @Html.DisplayNameFor(model => model.Endereco)
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.Endereco)
                </dd>
            </dl>
        </div>

        <div onload="">

            <div id="map_canvas" style="width: 100%; height: 432px">
            </div>
        </div>
    </div>
</div>